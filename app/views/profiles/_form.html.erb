<%= simple_form_for(@profile) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name, placeholder: "Enter your name" %>
    <%= f.input :dob %>
    <%= f.input :location %>
    <%= f.input :bio %>
    <%= f.input :picture , as: :file %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
